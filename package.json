    "classnames": "^2.3.2",
    "debounce-promise": "^3.1.2",
    "http-proxy-middleware": "^2.0.6",
    "lodash": "^4.17.21",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^7.2.6",
    "react-router-dom": "^6.4.3",
    "sass": "^1.55.0",
    "vite-jsconfig-paths": "^2.0.1"
